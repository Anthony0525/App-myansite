CharacterDetail.vue'\r\n\r\n// Estado global en la vista\r\nconst characters = ref([])\r\nconst showModal = ref(false)\r\nconst selectedCharacter = ref(null)\r\nconst episodes = ref([])\r\n\r\n// Obtener personajes\r\nconst getCharacters = async () => {\r\n  try {\r\n    const response = await axios.get('https://rickandmortyapi.com/api/character')\r\n    characters.value = response.data.results\r\n  } catch (error) {\r\n    console.error('Error al obtener personajes:', error)\r\n  }\r\n}\r\n\r\n// Mostrar detalle\r\nconst showDetail = async (character) => {\r\n  selectedCharacter.value = character\r\n  showModal.value = true\r\n  try {\r\n    const episodeRequests = character.episode.slice(0, 5).map(url => axios.get(url))\r\n    const responses = await Promise.all(episodeRequests)\r\n    episodes.value = responses.map(r => r.data)\r\n  } catch (error) {\r\n    console.error('Error al obtener episodios:', error)\r\n  }\r\n}\r\n\r\n// Cerrar modal\r\nconst closeModal = () => {\r\n  showModal.value = false\r\n  selectedCharacter.value = null\r\n  episodes.value = []\r\n}\r\n\r\nonMounted(getCharacters)\r\n</script>\r\n"],"mappings":";;uBACEA,mBAAA,CAeM,cAdJC,mBAAA,2BAA8B,E,CAErBC,MAAA,CAAAC,SAAS,I,cADlBC,YAAA,CAIEF,MAAA;IAPNG,GAAA;IAKOC,UAAU,EAAEJ,MAAA,CAAAI,UAAU;IACtBC,iBAAgB,EAAEL,MAAA,CAAAM;6CANzBP,mBAAA,gBASIA,mBAAA,sBAAyB,EACzBQ,YAAA,CAKEP,MAAA;IAJCQ,IAAI,EAAER,MAAA,CAAAC,SAAS;IACfQ,SAAS,EAAET,MAAA,CAAAU,iBAAiB;IAC5BC,QAAQ,EAAEX,MAAA,CAAAW,QAAQ;IAClBC,OAAK,EAAEZ,MAAA,CAAAa","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}