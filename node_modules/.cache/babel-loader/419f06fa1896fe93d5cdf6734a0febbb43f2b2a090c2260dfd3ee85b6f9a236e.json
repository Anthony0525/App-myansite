;\r\n  height: 50px;\r\n  background: rgba(0, 60, 119, 0.35); /* transparente */\r\n  backdrop-filter: blur(10px);\r\n  -webkit-backdrop-filter: blur(10px); /* Safari */\r\n  z-index: 1000;\r\n  display: flex;\r\n  align-items: center; /* centra verticalmente */\r\n}\r\n\r\n.navbar {\r\n  width: 100%;\r\n  display: flex;\r\n  justify-content: center;\r\n}\r\n\r\n.navbar ul {\r\n  list-style: none;\r\n  display: flex;\r\n  gap: 2rem;\r\n  margin: 0;\r\n  padding: 0;\r\n}\r\n\r\n.navbar a {\r\n  color: #fff;\r\n  text-decoration: none;\r\n  font-weight: bold;\r\n  transition: color 0.2s ease-in-out;\r\n}\r\n\r\n.navbar a:hover {\r\n  color: #ffeb3b; /* opcional: efecto hover */\r\n}\r\n\r\n/* ðŸ”¹ Empuja el contenido sin cortar cards */\r\n.main {\r\n  flex: 1;\r\n  padding-inline: 1rem;\r\n  box-sizing: border-box;\r\n}\r\n</style>\r\n    .then((result) => {\r\n      if (error) {\r\n        console.error(\"Error al cargar publicaciones:\", error)\r\n      } else {\r\n        publicaciones.value.push(...(result.data || []))\r\n        pagina.value++\r\n      }\r\n      cargando.value = false\r\n    })\r\n}"],"mappings":"AAuBA,SAASA,OAAO,EAAEC,SAAS,EAAEC,UAAU,EAAEC,UAAU,QAAQ,YAAY","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}