    class: \"close\",\n    onClick: _cache[0] || (_cache[0] = (...args) => $options.closeModal && $options.closeModal(...args))\n  }, \"×\"), _createElementVNode(\"img\", {\n    id: \"modalImg\",\n    src: $options.selectedAnimeImage,\n    alt: \"Anime\"\n  }, null, 8 /* PROPS */, _hoisted_6), _createElementVNode(\"h2\", _hoisted_7, _toDisplayString($data.selectedAnime?.title), 1 /* TEXT */), _createElementVNode(\"p\", _hoisted_8, _toDisplayString($data.selectedAnime?.synopsis || 'Sinopsis no disponible.'), 1 /* TEXT */), _createElementVNode(\"p\", null, [_cache[1] || (_cache[1] = _createElementVNode(\"strong\", null, \"Tipo:\", -1 /* HOISTED */)), _cache[2] || (_cache[2] = _createTextVNode()), _createElementVNode(\"span\", _hoisted_9, _toDisplayString($data.selectedAnime?.type), 1 /* TEXT */)]), _createElementVNode(\"p\", null, [_cache[3] || (_cache[3] = _createElementVNode(\"strong\", null, \"Episodios:\", -1 /* HOISTED */)), _cache[4] || (_cache[4] = _createTextVNode()), _createElementVNode(\"span\", _hoisted_10, _toDisplayString($data.selectedAnime?.episodes || 'Desconocido'), 1 /* TEXT */)]), _createElementVNode(\"p\", null, [_cache[5] || (_cache[5] = _createElementVNode(\"strong\", null, \"Año:\", -1 /* HOISTED */)), _cache[6] || (_cache[6] = _createTextVNode()), _createElementVNode(\"span\", _hoisted_11, _toDisplayString($data.selectedAnime?.year || 'N/A'), 1 /* TEXT */)])])], 4 /* STYLE */)]);\n}","map":{"version":3,"names":["class","id","_createElementBlock","_hoisted_1","_createCommentVNode","_createElementVNode","_hoisted_2","_Fragment","_renderList","$data","animes","anime","key","onClick","$event","$options","showDetail","src","images","jpg","image_url","alt","title","_hoisted_4","_toDisplayString","_hoisted_3","style","_normalizeStyle","display","showModal","_hoisted_5","_cache","args","closeModal","selectedAnimeImage","_hoisted_6","_hoisted_7","selectedAnime","_hoisted_8","synopsis","_createTextVNode","_hoisted_9","type","_hoisted_10","episodes","_hoisted_11","year"],"sources":["C:\\Users\\ellen\\Downloads\\proyectoxample\\proyectoxample\\src\\views\\Anime.vue"],"sourcesContent":["<template>\r\n  <div class=\"anime-view\">\r\n    <!-- Contenedor de cards -->\r\n    <div id=\"charactersContainer\" class=\"characters-container\">\r\n      <div \r\n        v-for=\"anime in animes\" \r\n        :key=\"anime.id\" \r\n        class=\"character-card\"\r\n        @click=\"showDetail(anime)\"\r\n      ><script>\r\nexport default {\r\n  name: 'AnimeView'  // Asegúrate de que sea AnimeView, no Anime\r\n}\r\n</script>\r\n        <img :src=\"anime.images?.jpg?.image_url || ''\" :alt=\"anime.title\" />\r\n        <h3>{{ anime.title }}</h3>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Modal para detalle de anime -->\r\n    <div id=\"animeModal\" class=\"modal\" :style=\"{ display: showModal ? 'flex' : 'none' }\">\r\n      <div class=\"modal-content\">\r\n        <span class=\"close\" @click=\"closeModal\">&times;</span>\r\n        <img id=\"modalImg\" :src=\"selectedAnimeImage\" alt=\"Anime\" />\r\n        <h2 id=\"modalTitle\">{{ selectedAnime?.title }}</h2>\r\n        <p id=\"modalSynopsis\">{{ selectedAnime?.synopsis || 'Sinopsis no disponible.' }}</p>\r\n        <p><strong>Tipo:</strong> <span id=\"modalType\">{{ selectedAnime?.type }}</span></p>\r\n        <p><strong>Episodios:</strong> <span id=\"modalEpisodes\">{{ selectedAnime?.episodes || 'Desconocido' }}</span></p>\r\n        <p><strong>Año:</strong> <span id=\"modalYear\">{{ selectedAnime?.year || 'N/A' }}</span></p>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'Anime',\r\n  data() {\r\n    return {\r\n      animes: [],\r\n      showModal: false,\r\n      selectedAnime: null\r\n    }\r\n  },\r\n  computed: {\r\n    selectedAnimeImage() {\r\n      if (!this.selectedAnime) return '';\r\n      return this.selectedAnime.images?.jpg?.large_image_url || \r\n             this.selectedAnime.images?.jpg?.image_url || '';\r\n    }\r\n  },\r\n  mounted() {\r\n    this.loadAnimes();\r\n    // Cerrar modal al hacer clic fuera\r\n    window.addEventListener('click', this.handleClickOutside);\r\n  },\r\n  beforeUnmount() {\r\n    window.removeEventListener('click', this.handleClickOutside);\r\n  },\r\n  methods: {\r\n    async loadAnimes() {\r\n      try {\r\n        // En un proyecto real, esto sería una llamada a una API\r\n        // Por ahora usamos los datos directamente\r\n        this.animes = [\r\n          {\r\n            \"id\": 5114,\r\n            \"title\": \"Fullmetal Alchemist: Brotherhood\",\r\n            \"year\": 2009,\r\n            \"episodes\": 64,\r\n            \"type\": \"TV\",\r\n            \"synopsis\": \"Dos hermanos alquimistas buscan la piedra filosofal para restaurar sus cuerpos después de un fallido intento de resucitar a su madre.\",\r\n            \"images\": {\r\n              \"jpg\": {\r\n                \"image_url\": \"https://cdn.myanimelist.net/images/anime/1208/94745.jpg\"\r\n              }\r\n            }\r\n          },\r\n          {\r\n            \"id\": 30276,\r\n            \"title\": \"One Punch Man\",\r\n            \"year\": 2015,\r\n            \"episodes\": 12,\r\n            \"type\": \"TV\",\r\n            \"synopsis\": \"Saitama, un héroe tan fuerte que derrota a cualquiera de un solo golpe, busca encontrar un oponente que le ofrezca un verdadero desafío.\",\r\n            \"images\": {\r\n              \"jpg\": {\r\n                \"image_url\": \"https://cdn.myanimelist.net/images/anime/12/76049.jpg\"\r\n              }\r\n            }\r\n          },\r\n          {\r\n            \"id\": 1,\r\n            \"title\": \"Cowboy Bebop\",\r\n            \"year\": 1998,\r\n            \"episodes\": 26,\r\n            \"type\": \"TV\",\r\n            \"synopsis\": \"La tripulación de la nave Bebop viaja por el espacio como cazarrecompensas mientras enfrentan sus pasados problemáticos.\",\r\n            \"images\": {\r\n              \"jpg\": {\r\n                \"image_url\": \"https://cdn.myanimelist.net/images/anime/4/19644.jpg\"\r\n              }\r\n            }\r\n          },\r\n          {\r\n            \"id\": 1535,\r\n            \"title\": \"Death Note\",\r\n            \"year\": 2006,\r\n            \"episodes\": 37,\r\n            \"type\": \"TV\",\r\n            \"synopsis\": \"Light Yagami encuentra un cuaderno que puede matar a cualquiera cuyo nombre se escriba en él, y decide usarlo para crear un mundo perfecto.\",\r\n            \"images\": {\r\n              \"jpg\": {\r\n                \"image_url\": \"https://cdn.myanimelist.net/images/anime/9/9453.jpg\"\r\n              }\r\n            }\r\n          },\r\n          {\r\n            \"id\": 40028,\r\n            \"title\": \"Kimetsu no Yaiba (Demon Slayer)\",\r\n            \"year\": 2019,\r\n            \"episodes\": 26,\r\n            \"type\": \"TV\",\r\n            \"synopsis\": \"Tanjiro Kamado se convierte en cazador de demonios tras la masacre de su familia y la transformación de su hermana en un demonio.\",\r\n            \"images\": {\r\n              \"jpg\": {\r\n                \"image_url\": \"https://cdn.myanimelist.net/images/anime/1286/99889.jpg\"\r\n              }\r\n            }\r\n          },\r\n          {\r\n            \"id\": 16498,\r\n            \"title\": \"Shingeki no Kyojin (Attack on Titan)\",\r\n            \"year\": 2013,\r\n            \"episodes\": 25,\r\n            \"type\": \"TV\",\r\n            \"synopsis\": \"La humanidad lucha por su supervivencia dentro de ciudades amuralladas contra titanes gigantes que devoran humanos.\",\r\n            \"images\": {\r\n              \"jpg\": {\r\n                \"image_url\": \"https://cdn.myanimelist.net/images/anime/10/47347.jpg\"\r\n              }\r\n            }\r\n          },\r\n          {\r\n            \"id\": 11061,\r\n            \"title\": \"Hunter x Hunter (2011)\",\r\n            \"year\": 2011,\r\n            \"episodes\": 148,\r\n            \"type\": \"TV\",\r\n            \"synopsis\": \"Gon Freecss sueña con convertirse en cazador para encontrar a su padre, quien lo abandonó cuando era niño.\",\r\n            \"images\": {\r\n              \"jpg\": {\r\n                \"image_url\": \"https://cdn.myanimelist.net/images/anime/1337/99013.jpg\"\r\n              }\r\n            }\r\n          },\r\n          {\r\n            \"id\": 9253,\r\n            \"title\": \"Steins;Gate\",\r\n            \"year\": 2011,\r\n            \"episodes\": 24,\r\n            \"type\": \"TV\",\r\n            \"synopsis\": \"Un grupo de amigos descubre cómo enviar mensajes al pasado, desencadenando eventos que cambian su presente.\",\r\n            \"images\": {\r\n              \"jpg\": {\r\n                \"image_url\": \"https://cdn.myanimelist.net/images/anime/1935/127974.jpg\"\r\n              }\r\n            }\r\n          },\r\n          {\r\n            \"id\": 28891,\r\n            \"title\": \"KonoSuba\",\r\n            \"year\": 2016,\r\n            \"episodes\": 10,\r\n            \"type\": \"TV\",\r\n            \"synopsis\": \"Kazuma muere y renace en un mundo de fantasía con una diosa inútil, formando un grupo de aventureros incompetentes.\",\r\n            \"images\": {\r\n              \"jpg\": {\r\n                \"image_url\": \"https://cdn.myanimelist.net/images/anime/8/77831.jpg\"\r\n              }\r\n            }\r\n          },\r\n          {\r\n            \"id\": 918,\r\n            \"title\": \"Gintama\",\r\n            \"year\": 2006,\r\n            \"episodes\": 201,\r\n            \"type\": \"TV\",\r\n            \"synopsis\": \"En un Japón alternativo dominado por alienígenas, un samurái lucha con humor y acción para sobrevivir en un mundo cambiante.\",\r\n            \"images\": {\r\n              \"jpg\": {\r\n                \"image_url\": \"https://cdn.myanimelist.net/images/anime/10/73274.jpg\"\r\n              }\r\n            }\r\n          },\r\n          {\r\n            \"id\": 6702,\r\n            \"title\": \"Fairy Tail\",\r\n            \"year\": 2009,\r\n            \"episodes\": 175,\r\n            \"type\": \"TV\",\r\n            \"synopsis\": \"Natsu Dragneel y sus amigos del gremio Fairy Tail viven aventuras mágicas mientras protegen a sus seres queridos.\",\r\n            \"images\": {\r\n              \"jpg\": {\r\n                \"image_url\": \"https://cdn.myanimelist.net/images/anime/5/18179.jpg\"\r\n              }\r\n            }\r\n          },\r\n          {\r\n            \"id\": 19815,\r\n            \"title\": \"No Game No Life\",\r\n            \"year\": 2014,\r\n            \"episodes\": 12,\r\n            \"type\": \"TV\",\r\n            \"synopsis\": \"Sora y Shiro son transportados a un mundo donde todo se decide con juegos, desafiando a la raza gobernante.\",\r\n            \"images\": {\r\n              \"jpg\": {\r\n                \"image_url\": \"https://cdn.myanimelist.net/images/anime/1074/111944.jpg\"\r\n              }\r\n            }\r\n          },\r\n          {\r\n            \"id\": 31964,\r\n            \"title\": \"Boku no Hero Academia (My Hero Academia)\",\r\n            \"year\": 2016,\r\n            \"episodes\": 13,\r\n            \"type\": \"TV\",\r\n            \"synopsis\": \"En un mundo donde la mayoría tiene superpoderes, Izuku Midoriya sueña con convertirse en un héroe a pesar de nacer sin poderes.\",\r\n            \"images\": {\r\n              \"jpg\": {\r\n                \"image_url\": \"https://cdn.myanimelist.net/images/anime/10/78745.jpg\"\r\n              }\r\n            }\r\n          },\r\n          {\r\n            \"id\": 23273,\r\n            \"title\": \"Shigatsu wa Kimi no Uso (Your Lie in April)\",\r\n            \"year\": 2014,\r\n            \"episodes\": 22,\r\n            \"type\": \"TV\",\r\n            \"synopsis\": \"Un pianista prodigio que perdió su pasión por la música conoce a una violinista que le ayuda a redescubrir su talento.\",\r\n            \"images\": {\r\n              \"jpg\": {\r\n                \"image_url\": \"https://cdn.myanimelist.net/images/anime/3/67177.jpg\"\r\n              }\r\n            }\r\n          },\r\n          {\r\n            \"id\": 20507,\r\n            \"title\": \"Haikyuu!!\",\r\n            \"year\": 2014,\r\n            \"episodes\": 25,\r\n            \"type\": \"TV\",\r\n            \"synopsis\": \"Hinata Shoyo se une al equipo de voleibol de su escuela para cumplir su sueño de convertirse en el mejor jugador.\",\r\n            \"images\": {\r\n              \"jpg\": {\r\n                \"image_url\": \"https://cdn.myanimelist.net/images/anime/7/76014.jpg\"\r\n              }\r\n            }\r\n          },\r\n          {\r\n            \"id\": 33352,\r\n            \"title\": \"Violet Evergarden\",\r\n            \"year\": 2018,\r\n            \"episodes\": 13,\r\n            \"type\": \"TV\",\r\n            \"synopsis\": \"Una ex soldado se convierte en escritora de cartas para entender las emociones humanas y el significado del amor.\",\r\n            \"images\": {\r\n              \"jpg\": {\r\n                \"image_url\": \"https://cdn.myanimelist.net/images/anime/1795/95088.jpg\"\r\n              }\r\n            }\r\n          },\r\n          {\r\n            \"id\": 38000,\r\n            \"title\": \"Kimetsu no Yaiba: Mugen Ressha-hen (Demon Slayer: Mugen Train)\",\r\n            \"year\": 2020,\r\n            \"episodes\": 1,\r\n            \"type\": \"Movie\",\r\n            \"synopsis\": \"Tanjiro y sus amigos se unen a uno de los pilares para investigar desapariciones en un tren misterioso.\",\r\n            \"images\": {\r\n              \"jpg\": {\r\n                \"image_url\": \"https://cdn.myanimelist.net/images/anime/1704/106947.jpg\"\r\n              }\r\n            }\r\n          },\r\n          {\r\n            \"id\": 21,\r\n            \"title\": \"One Piece\",\r\n            \"year\": 1999,\r\n            \"episodes\": 1000,\r\n            \"type\": \"TV\",\r\n            \"synopsis\": \"Monkey D. Luffy y su tripulación navegan por Grand Line en busca del tesoro definitivo, el One Piece.\",\r\n            \"images\": {\r\n              \"jpg\": {\r\n                \"image_url\": \"https://cdn.myanimelist.net/images/anime/6/73245.jpg\"\r\n              }\r\n            }\r\n          },\r\n          {\r\n            \"id\": 11757,\r\n            \"title\": \"Sword Art Online\",\r\n            \"year\": 2012,\r\n            \"episodes\": 25,\r\n            \"type\": \"TV\",\r\n            \"synopsis\": \"Jugadores quedan atrapados en un juego de realidad virtual donde la muerte en el juego significa la muerte real.\",\r\n            \"images\": {\r\n              \"jpg\": {\r\n                \"image_url\": \"https://cdn.myanimelist.net/images/anime/11/39717.jpg\"\r\n              }\r\n            }\r\n          },\r\n          {\r\n            \"id\": 40748,\r\n            \"title\": \"Jujutsu Kaisen\",\r\n            \"year\": 2020,\r\n            \"episodes\": 24,\r\n            \"type\": \"TV\",\r\n            \"synopsis\": \"Yuji Itadori se convierte en hechicero después de tragar un objeto maldito para salvar a sus amigos.\",\r\n            \"images\": {\r\n              \"jpg\": {\r\n                \"image_url\": \"https://cdn.myanimelist.net/images/anime/1171/109222.jpg\"\r\n              }\r\n            }\r\n          }\r\n        ];\r\n      } catch (err) {\r\n        console.error(\"Error cargando animes:\", err);\r\n      }\r\n    },\r\n    showDetail(anime) {\r\n      this.selectedAnime = anime;\r\n      this.showModal = true;\r\n    },\r\n    closeModal() {\r\n      this.showModal = false;\r\n      this.selectedAnime = null;\r\n    },\r\n    handleClickOutside(event) {\r\n      if (event.target === document.getElementById('animeModal')) {\r\n        this.closeModal();\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.anime-view {\r\n  padding: 20px;\r\n}\r\n\r\n.characters-container {\r\n  display: grid;\r\n  grid-template-columns: repeat(2, 1fr);\r\n  gap: 20px;\r\n  justify-content: space-between;\r\n  padding: 40px;\r\n}\r\n\r\n.character-card {\r\n  border: 5px solid #ccc;\r\n  border-radius: 8px;\r\n  padding: 10px;\r\n  margin-bottom: 10px;\r\n  background-color: #acacac;\r\n  text-align: center;\r\n  box-shadow: 5px 5px 15px rgba(0, 0, 0, 0.3);\r\n  width: auto;\r\n  height: auto;\r\n  font-family: 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;\r\n  cursor: pointer;\r\n  transition: transform 0.4s, background-color 0.4s;\r\n}\r\n\r\n.character-card:hover {\r\n  transform: scale(0.95);\r\n  background-color: #555454;\r\n}\r\n\r\n.character-card img {\r\n  width: 90%;\r\n  height: auto;\r\n  border-radius: 8px;\r\n}\r\n\r\n/* Modal estilos */\r\n.modal {\r\n  display: none;\r\n  position: fixed;\r\n  z-index: 999;\r\n  left: 0; top: 0;\r\n  width: 100%; height: 100%;\r\n  background: rgba(0,0,0,0.7);\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.modal-content {\r\n  background: #fff;\r\n  border-radius: 10px;\r\n  padding: 2rem;\r\n  max-width: 400px;\r\n  width: 90%;\r\n  text-align: center;\r\n  position: relative;\r\n  box-shadow: 0 2px 8px rgba(0,0,0,0.2);\r\n  color: #333;\r\n}\r\n\r\n.close {\r\n  position: absolute;\r\n  top: 1rem;\r\n  right: 1rem;\r\n  font-size: 2rem;\r\n  color: #e74c3c;\r\n  cursor: pointer;\r\n}\r\n\r\n.modal-content img {\r\n  width: 200px;\r\n  border-radius: 10px;\r\n  margin-bottom: 15px;\r\n}\r\n\r\n.modal-content h2 {\r\n  color: #42b983;\r\n  margin-bottom: 15px;\r\n}\r\n\r\n.modal-content p {\r\n  margin-bottom: 10px;\r\n  text-align: left;\r\n}\r\n\r\n.modal-content strong {\r\n  color: #42b983;\r\n}\r\n\r\n@media (min-width: 1024px) {\r\n  .characters-container {\r\n    grid-template-columns: repeat(4, 1fr);\r\n    justify-content: space-between;\r\n  }\r\n}\r\n\r\n@media (max-width: 768px) {\r\n  .characters-container {