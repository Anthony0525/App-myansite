{"ast":null,"code":"import { IonPage, IonHeader, IonToolbar, IonContent } from \"@ionic/vue\";\nimport { ref } from \"vue\";\nexport default {\n  __name: 'MainLayout',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const menuOpen = ref(false);\n    const toggleMenu = () => {\n      menuOpen.value = !menuOpen.value;\n    };\n    const __returned__ = {\n      menuOpen,\n      toggleMenu,\n      get IonPage() {\n        return IonPage;\n      },\n      get IonHeader() {\n        return IonHeader;\n      },\n      get IonToolbar() {\n        return IonToolbar;\n      },\n      get IonContent() {\n        return IonContent;\n      },\n      ref\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["IonPage","IonHeader","IonToolbar","IonContent","ref","menuOpen","toggleMenu","value"],"sources":["C:/Users/estudiante/Downloads/Myansite Ionic/Myansite/Myansite/src/layouts/MainLayout.vue"],"sourcesContent":["<template>\r\n  <IonPage>\r\n    <!-- Header / Navbar -->\r\n    <IonHeader class=\"header\">\r\n      <IonToolbar class=\"navbar\">\r\n        <!-- Botón hamburguesa -->\r\n        <button class=\"hamburger\" @click=\"toggleMenu\">\r\n          ☰\r\n        </button>\r\n\r\n        <!-- Menú de navegación -->\r\n        <ul :class=\"{ open: menuOpen }\">\r\n          <li><router-link class=\"nav-link\" to=\"/app/contenido\">Inicio</router-link></li>\r\n          <li><router-link class=\"nav-link\" to=\"#\">Explorar</router-link></li>\r\n          <li><router-link class=\"nav-link\" to=\"#\">Chats</router-link></li>\r\n          <li><router-link class=\"nav-link\" to=\"#\">Agregar</router-link></li>\r\n          <li><router-link class=\"nav-link\" to=\"/app/perfil\">Perfil</router-link></li>\r\n          <li><router-link class=\"nav-link\" to=\"#\">Ajustes</router-link></li>\r\n        </ul>\r\n      </IonToolbar>\r\n    </IonHeader>\r\n\r\n    <!-- Contenido dinámico -->\r\n    <IonContent class=\"main\">\r\n      <router-view />\r\n    </IonContent>\r\n  </IonPage>\r\n</template>\r\n\r\n<script setup>\r\nimport { IonPage, IonHeader, IonToolbar, IonContent } from \"@ionic/vue\"\r\nimport { ref } from \"vue\"\r\n\r\nconst menuOpen = ref(false)\r\nconst toggleMenu = () => {\r\n  menuOpen.value = !menuOpen.value\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n/* ===== Fondo del contenido ===== */\r\n:deep(.main) {\r\n  --background: rgba(15, 15, 15, 0.9) !important;\r\n  backdrop-filter: blur(12px);\r\n  -webkit-backdrop-filter: blur(12px);\r\n  min-height: 100vh;\r\n  width: 100%;\r\n  padding-top: 80px;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n/* ===== Navbar con blur ===== */\r\n:deep(.header),\r\n:deep(.navbar) {\r\n  --background: rgba(15, 15, 15, 0.75) !important;\r\n  --color: #fff;\r\n  backdrop-filter: blur(12px);\r\n  -webkit-backdrop-filter: blur(12px);\r\n  border-bottom: 1px solid rgba(255, 255, 255, 0.15);\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  padding: 0 1rem;\r\n  position: relative;\r\n  z-index: 10;\r\n}\r\n\r\n/* ===== Botón hamburguesa ===== */\r\n.hamburger {\r\n  display: none;\r\n  background: none;\r\n  color: white;\r\n  font-size: 1.5rem;\r\n  border: none;\r\n  cursor: pointer;\r\n}\r\n\r\n/* ===== Menú ===== */\r\n:deep(.navbar ul) {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  gap: 2rem;\r\n  padding: 0;\r\n  margin: 0;\r\n  list-style: none;\r\n  transition: max-height 0.3s ease-in-out;\r\n}\r\n\r\n/* ===== Links ===== */\r\n:deep(.nav-link) {\r\n  font-weight: 600;\r\n  font-size: 1.1rem;\r\n  text-decoration: none;\r\n  padding: 6px 12px;\r\n  border-radius: 6px;\r\n  transition: 0.3s ease;\r\n  color: white;\r\n}\r\n\r\n:deep(.nav-link:hover) {\r\n  color: #3d0065 !important;\r\n  background: rgba(255, 255, 255, 0.1);\r\n}\r\n\r\n/* ===== Activo ===== */\r\n:deep(.router-link-exact-active) {\r\n  color: #c409e6 !important;\r\n  font-weight: 700;\r\n}\r\n\r\n/* ===== Responsive Styles ===== */\r\n@media (max-width: 768px) {\r\n  .hamburger {\r\n    display: block;\r\n  }\r\n\r\n  :deep(.navbar ul) {\r\n    flex-direction: column;\r\n    align-items: flex-start;\r\n    position: absolute;\r\n    top: 56px;\r\n    left: 0;\r\n    width: 100%;\r\n    background: rgba(15, 15, 15, 0.95);\r\n    max-height: 0;\r\n    overflow: hidden;\r\n    padding: 0 1rem;\r\n    border-top: 1px solid rgba(255, 255, 255, 0.15);\r\n  }\r\n\r\n  :deep(.navbar ul.open) {\r\n    max-height: 300px; /* Suficiente para todos los links */\r\n    padding: 1rem;\r\n  }\r\n\r\n  :deep(.navbar ul li) {\r\n    width: 100%;\r\n    margin-bottom: 1rem;\r\n  }\r\n\r\n  :deep(.nav-link) {\r\n    display: block;\r\n    width: 100%;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":"AA8BA,SAASA,OAAO,EAAEC,SAAS,EAAEC,UAAU,EAAEC,UAAU,QAAQ,YAAY;AACvE,SAASC,GAAG,QAAQ,KAAK;;;;;;;IAEzB,MAAMC,QAAQ,GAAGD,GAAG,CAAC,KAAK,CAAC;IAC3B,MAAME,UAAU,GAAGA,CAAA,KAAM;MACvBD,QAAQ,CAACE,KAAK,GAAG,CAACF,QAAQ,CAACE,KAAK;IAClC,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}