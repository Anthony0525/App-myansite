s.selectedCharacter = character;\n      this.episodes = [];\n\n      try {\n        const episodeData = await Promise.all(\n          character.episode.map((url) => fetch(url).then((r) => r.json()))\n        );\n        this.episodes = episodeData;\n      } catch (error) {\n        console.error(\"‚ùå Error cargando episodios:\", error);\n      }\n    }\n  },\n  mounted() {\n    this.fetchCharacters(); // üîπ Llamar a la API cuando se monte el componente\n  }\n};\n</script>\n"],"mappings":";;AAwDA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,EAAE;MAAW;MACzBC,iBAAiB,EAAE,IAAI;MAAE;MACzBC,QAAQ,EAAE,EAAC,CAAc;IAC3B,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,eAAeA,CAAA,EAAG;MACtB,IAAI;QACF,MAAMC,GAAE,GAAI,MAAMC,KAAK,CAAC,2CAA2C,CAAC;QACpE,MAAMP,IAAG,GAAI,MAAMM,GAAG,CAACE,IAAI,CAAC,CAAC;QAC7B,IAAI,CAACP,UAAS,GAAID,IAAI,CAACS,OAAO,EAAE;MAClC,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,8BAA8B,EAAEA,KAAK,CAAC;MACtD;IACF,CAAC;IACD,MAAME,eAAeA,CAACC,SAAS,EAAE;MAC/B,IAAI,CAACX,iBAAgB,GAAIW,SAAS;MAClC,IAAI,CAACV,QAAO,GAAI,EAAE;MAElB,IAAI;QACF,MAAMW,WAAU,GAAI,MAAMC,OAAO,CAACC,GAAG,CACnCH,SAAS,CAACI,OAAO,CAACC,GAAG,CAAEC,GAAG,IAAKZ,KAAK,CAACY,GAAG,CAAC,CAACC,IAAI,CAAEC,CAAC,IAAKA,CAAC,CAACb,IAAI,CAAC,CAAC,CAAC,CACjE,CAAC;QACD,IAAI,CAACL,QAAO,GAAIW,WAAW;MAC7B,EAAE,OAAOJ,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,6BAA6B,EAAEA,KAAK,CAAC;MACrD;IACF;EACF,CAAC;EACDY,OAAOA,CAAA,EAAG;IACR,IAAI,CAACjB,eAAe,CAAC,CAAC,EAAE;EAC1B;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}