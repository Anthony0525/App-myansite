selectedAnime.value) return ''\r\n  return selectedAnime.value.images?.jpg?.large_image_url || \r\n         selectedAnime.value.images?.jpg?.image_url || ''\r\n})\r\n\r\nconst loadAnimes = async () => {\r\n  try {\r\n    // 🔹 Puedes cambiar esta parte para traerlos de un archivo JSON o API\r\n    animes.value = [] \r\n  } catch (err) {\r\n    console.error(\"Error cargando animes:\", err)\r\n  }\r\n}\r\n\r\nconst showDetail = (anime) => {\r\n  selectedAnime.value = anime\r\n  showModal.value = true\r\n}\r\n\r\nconst closeModal = () => {\r\n  showModal.value = false\r\n  selectedAnime.value = null\r\n}\r\n\r\nconst handleClickOutside = (event) => {\r\n  if (event.target === document.getElementById('animeModal')) {\r\n    closeModal()\r\n  }\r\n}\r\n\r\nonMounted(() => {\r\n  loadAnimes()\r\n  window.addEventListener('click', handleClickOutside)\r\n})\r\n\r\nonBeforeUnmount(() => {\r\n  window.removeEventListener('click', handleClickOutside)\r\n})\r\n</script>\r\n"],"mappings":"AA+BA,SAASA,GAAG,EAAEC,QAAQ,EAAEC,SAAS,EAAEC,eAAe,QAAQ,KAAK;AAC/D,OAAO,oBAAoB;;;;;;;IAE3B,MAAMC,MAAM,GAAGJ,GAAG,CAAC,EAAE,CAAC,EAAC;IACvB,MAAMK,SAAS,GAAGL,GAAG,CAAC,KAAK,CAAC;IAC5B,MAAMM,aAAa,GAAGN,GAAG,CAAC,IAAI,CAAC;IAE/B,MAAMO,kBAAkB,GAAGN,QAAQ,CAAC,MAAM;MACxC,IAAI,CAACK,aAAa,CAACE,KAAK,EAAE,OAAO,EAAE;MACnC,OAAOF,aAAa,CAACE,KAAK,CAACC,MAAM,EAAEC,GAAG,EAAEC,eAAe,IAChDL,aAAa,CAACE,KAAK,CAACC,MAAM,EAAEC,GAAG,EAAEE,SAAS,IAAI,EAAE;IACzD,CAAC,CAAC;IAEF,MAAMC,UAAU,GAAG,MAAAA,CAAA,KAAY;MAC7B,IAAI;QACF;QACAT,MAAM,CAACI,KAAK,GAAG,EAAE;MACnB,CAAC,CAAC,OAAOM,GAAG,EAAE;QACZC,OAAO,CAACC,KAAK,CAAC,wBAAwB,EAAEF,GAAG,CAAC;MAC9C;IACF,CAAC;IAED,MAAMG,UAAU,GAAIC,KAAK,IAAK;MAC5BZ,aAAa,CAACE,KAAK,GAAGU,KAAK;MAC3Bb,SAAS,CAACG,KAAK,GAAG,IAAI;IACxB,CAAC;IAED,MAAMW,UAAU,GAAGA,CAAA,KAAM;MACvBd,SAAS,CAACG,KAAK,GAAG,KAAK;MACvBF,aAAa,CAACE,KAAK,GAAG,IAAI;IAC5B,CAAC;IAED,MAAMY,kBAAkB,GAAIC,KAAK,IAAK;MACpC,IAAIA,KAAK,CAACC,MAAM,KAAKC,QAAQ,CAACC,cAAc,CAAC,YAAY,CAAC,EAAE;QAC1DL,UAAU,CAAC,CAAC;MACd;IACF,CAAC;IAEDjB,SAAS,CAAC,