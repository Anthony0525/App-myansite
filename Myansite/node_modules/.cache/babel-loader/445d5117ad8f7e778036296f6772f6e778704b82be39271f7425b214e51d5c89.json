der class=\"hero\" role=\"banner\" aria-label=\"Fondo Myansite\"></header>\r\n\r\n  <main class=\"card\" role=\"main\">\r\n    <h1>Bienvenido!!!</h1>\r\n    <p class=\"lead\">Myansite un lugar único donde el anime vive</p>\r\n\r\n    <div class=\"btn-group\" role=\"navigation\" aria-label=\"Acciones principales\">\r\n      <button class=\"btn\" @click=\"mostrarLogin\">Iniciar sesión!</button>\r\n      <button class=\"btn\" @click=\"mostrarRegistro\">Registro!</button>\r\n      <button class=\"btn\" @click=\"mostrarNoticias\">Noticias Anime!</button>\r\n    </div>\r\n\r\n    <!-- Aquí cambia el contenido dinámico -->\r\n    <div v-html=\"contenido\"></div>\r\n  </main>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, onMounted } from \"vue\"\r\nimport axios from \"axios\"\r\nimport \"@/assets/css/styles.css\"\r\nimport { createClient } from \"@supabase/supabase-js\"\r\n\r\nconst supabaseUrl = \"https://neffqwnqpcydidpkysfi.supabase.co\"\r\nconst supabaseKey = \"TU_API_KEY\" // ⚠️ reemplaza por la tuya\r\nconst supabase = createClient(supabaseUrl, supabaseKey)\r\n\r\nconst contenido = ref(\"\")\r\n\r\nfunction mostrarRegistro() {\r\n  contenido.value = `\r\n    <h1>Registro</h1>\r\n    <form @submit.prevent=\"registrar\">\r\n      <input type=\"text\" placeholder=\"Usuario\" v-model=\"usuario\"/>\r\n      <input type=\"email\" placeholder=\"Correo\" v-model=\"correo\"/>\r\n      <input type=\"password\" placeholder=\"Contraseña\" v-model=\"contrasena\"/>\r\n      <button type=\"submit\">Registrarse</button>\r\n    </form>\r\n  `\r\n}\r\n\r\nasync function registrar() {\r\n  const { error } = await supabase.from(\"usuarios\").insert([{ usuario, correo, contrasena }])\r\n  if (error) alert(\"Error: \" + error.message)\r\n  else alert(\"Usuario registrado ✅\")\r\n}\r\n\r\nfunction mostrarLogin() {\r\n  contenido.value = `\r\n    <h1>Login</h1>\r\n    <form @submit.prevent=\"login\">\r\n      <input type=\"email\" v-model=\"correo\" placeholder=\"Correo\"/>\r\n      <input type=\"password\" v-model=\"contrasena\" placeholder=\"Contraseña\"/>\r\n      <button type=\"submit\">Entrar</button>\r\n    </form>\r\n  `\r\n}\r\n\r\nasync function login() {\r\n  const { data, error } = await supabase.from(\"usuarios\").select(\"*\").eq(\"correo\", correo).maybeSingle()\r\n  if (error) alert(error.message)\r\n  else if (!data) alert(\"Usuario no encontrado\")\r\n  else if (data.contrasena === contrasena) {\r\n    localStorage.setItem(\"usuario\", JSON.stringify(data))\r\n    window.location.href = \"/contenido\"\r\n  } else alert(\"Contraseña incorrecta\")\r\n}\r\n\r\nasync function mostrarNoticias() {\r\n  try {\r\n    const res = await axios.get(\"https://api.jikan.moe/v4/top/anime?limit=10\")\r\n    contenido.value = res.data.data.map(anime => `\r\n      <div class=\"anime-card\">\r\n        <img src=\"${anime.images.jpg.large_image_url}\" alt=\"${anime.title}\"/>\r\n        <h3>${anime.title}</h3>\r\n      </div>\r\n    `).join(\"\")\r\n  } catch (err) {\r\n    contenido.value = \"<p>Error cargando noticias 😢</p>\"\r\n  }\r\n}\r\n</script>\r\n"],"mappings":";;EAGQA,KAAK,EAAC,MAAM;EAACC,IAAI,EAAC;;mBAH1B;;uBAAAC,mBAAA,CAAAC,SAAA,S,0BACEC,mBAAA,CAAwE;IAAhEJ,KAAK,EAAC,MAAM;IAACC,IAAI,EAAC,QAAQ;IAAC,YAAU,EAAC;+BAE9CG,mBAAA,CAYO,QAZPC,UAYO,G,0BAXLD,mBAAA,CAAsB,YAAlB,eAAa,sB,0BACjBA,mBAAA,CAA+D;IAA5DJ,KAAK,EAAC;EAAM,GAAC,6CAA2C,sBAE3DI,mBAAA,CAIM;IAJDJ,KAAK,EAAC,WAAW;IAACC,IAAI,EAAC,YAAY;IAAC,YAAU,EAAC;MAClDG,mBAAA,CAAkE;IAA1DJ,KAAK,EAAC,KAAK;IAAEM,OAAK,EAAEC,MAAA,CAAAC;KAAc,iBAAe,GACzDJ,mBAAA,CAA+D;IAAvDJ,KAAK,EAAC,KAAK;IAAEM,OAAK,EAAEC,MAAA,CAAAE;KAAiB,WAAS,GACtDL,mBAAA,CAAqE;IAA7DJ,KAAK,EAAC,KAAK;IAAEM,OAAK,EAAEC,MAAA,CAAAG;KAAiB,iBAAe,E,GAG9DC,mBAAA,uCAA0C,EAC1CP,mBAAA,CAA8B;IAAzBQ,SAAkB,EAAVL,MAAA,CAAAM;EAAS,wBAd1BC,UAAA,E","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}